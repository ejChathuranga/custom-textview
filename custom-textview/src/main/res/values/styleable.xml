<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DashTextView">
        <attr name="title" format="string"/>
        <attr name="tips" format="string"/>
        <attr name="color" format="color"/>
        <attr name="dash" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="underlineWidth" format="float" />
        <attr name="ctvUnderlineColor" format="color" />
        <attr name="underlinePadding" format="integer"/>
        <attr name="ctvUnderLineDotWidth" format="integer"/>
        <attr name="ctvUnderLineDotSpace" format="integer"/>
    </declare-styleable>
</resources>